	border-image:url(:/resource/res/image/back1.jpg);
}
*/




/*中色*/
QStatusBar{
	background:rgb(70, 80, 110);
	color:rgb(255,255,255);
}

QStatusBar::item{border: 0px}

QMenuBar{
	background:rgb(80, 90, 120);
	color:rgb(255,255,255);
}

QToolBar{
	spacing:8px;
	background:rgb(80, 90, 120);
	border: 15px;
}

QToolBar::separator{
	background-color:rgbs(100, 100, 130,0);
}


#SunnyTcsMapEditor{
	background-color:rgb(80, 90, 120);
}

QDockWidget{
	background:rgb(80, 90, 120);
	color:rgb(255,255,255);
}


QToolButton{
	background:rgb(80, 90, 120);
	color:rgv(255,255,255);
}


/*明色*/
QGraphicsView{
	background-color:rgb(80, 90, 120);
	border-color:rgb(80, 90, 120);
	border: 0px;
}



QTreeWidget{
	background:rgb(90, 100, 130);
	color:rgb(255,255,255);
	border-color:rgb(90, 100, 130);
	border: 3px;
}


QTableWidget{
	background:rgb(90, 100, 130);
	color:rgb(255,255,255);
	border-color:rgb(90, 100, 130);
	border: 3px;
}



QHeaderView::section{
	background-color:rgb(90, 100, 130);
	color:rgb(255,255,255);
}



QListWidget{
	background:rgb(90, 100, 130);
	color:rgb(255,255,255);
	border-color:rgb(90, 100, 130);
	border: 3px;
}


QLineEdit{
	background:rgb(90, 100, 130);
	color:rgb(255,255,255);
	border-color:rgb(90, 100, 130);
	border: 3px;
}


QScrollBar{
	background:rgb(100,110,140);
	border-color:rgb(80,90,120);
	color:rgb(80,90,120);
	border:0px;
}


QScrollBar::add-page,QScrollBar::sub-page
{
	background-color:rgb(80,90,120);
	border-color:rgb(80,90,120);
	color:rgb(80,90,120);
	border:0px;
}


